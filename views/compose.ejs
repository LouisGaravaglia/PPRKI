<%- include("partials/header"); %>


<div class="videosBox">


<div class="videosContainer">

  <h3 class="form-header" >Image Upload Page</h3>

  <form class="" action="/imageSubmitForm" method="post">
    <div class="form-group">
      <label class="form-label" for="exampleInputEmail1">Image File Name</label>
      <input name="imageLink" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="form-group">
      <label class="form-label" for="exampleInputEmail1">Image Date</label>
      <input name="imageDate" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="form-group">
      <label class="form-label" for="exampleInputEmail1">Description</label>
      <textarea class="form-control" cols="20" rows="3" aria-label="With textarea" name="imageInfo"></textarea>
    </div>
    <button type="submit" class="btn btn-primary form-button">Upload This Image</button>
  </form>

</div>

<div class="deleteContainer">

    <h3 class="form-header" >Delete Image</h3>

  <form action="/deleteImage" method="post">
    <button name="butt" value='2' class="btn btn-primary btn-danger form-button">Delete Image with Title of:</button>
    <input type="text" name="deletedImage" value="">
  </form>
</div>





</div>
